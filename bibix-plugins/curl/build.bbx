import bibix
import git("https://github.com/Joonsoo/bibix.git", tag="0.0.1", path="bibix-plugins/ktjvm") as ktjvm
import maven

impl = ktjvm.library(
  srcs = glob("src/**/*.kt"),
  deps = [
    bibix.base(),
    maven.dep("org.jetbrains.kotlin", "kotlin-stdlib-jdk8", "1.6.10"),
  ],
)

def download(
  url: string,
  filename?: string,
): file = impl:com.giyeok.bibix.plugins.curl.Download
